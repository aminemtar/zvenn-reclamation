<main>

{% set food=0 %}
    {% set price=0 %}
    {% set service=0 %}
    {% set s=0 %}
    <div class="container margin_30_20">
        <div class="row">
            <div class="col-lg-8 list_menu">
                <section id="section-5">
                    <h4>Reviews</h4>
                    <div class="row add_bottom_30 d-flex align-items-center reviews">
                        <div class="col-md-3">
                            <div id="review_summary">
                                {% for t in reclamation %}
                                    {% set food = food + t.foodqulaite%}
                                    {% set price = price + t.price%}
                                    {% set service = price + t.service%}
                                    {% set s=s+1 %}

                                {% endfor %}
                                <strong>{{ (((food/s)+(price/s)+(service/s))/3)|number_format('2') }}</strong>
                                <em>Superb</em>
                                <small>Based on 3 reviews</small>
                            </div>
                        </div>
                        <div class="col-md-9 reviews_sum_details">
                            <div class="row">
                                <div class="col-md-6">
                                    <h6>Food Quality</h6>
                                    <div class="row">
                                        <div class="col-xl-10 col-lg-9 col-9">
                                            <div class="progress">
                                                <div class="progress-bar" role="progressbar" style="width:{{ (food/s)*10 }}%" aria-valuenow="" aria-valuemin="0" aria-valuemax="100"></div>
                                            </div>
                                        </div>
                                        <div class="col-xl-2 col-lg-3 col-3"><strong>{{ (food/s)|number_format('1') }}</strong></div>
                                    </div>
                                    <!-- /row -->
                                    <h6>Service</h6>
                                    <div class="row">
                                        <div class="col-xl-10 col-lg-9 col-9">
                                            <div class="progress">
                                                <div class="progress-bar" role="progressbar" style="width: {{ (service/s)*10 }}%" aria-valuenow="" aria-valuemin="0" aria-valuemax="100"></div>
                                            </div>
                                        </div>
                                        <div class="col-xl-2 col-lg-3 col-3"><strong>{{ service/s|number_format('1')  }}</strong></div>
                                    </div>
                                    <!-- /row -->
                                </div>
                                <div class="col-md-6">
                                    <!-- /row -->
                                    <h6>Price</h6>
                                    <div class="row">
                                        <div class="col-xl-10 col-lg-9 col-9">
                                            <div class="progress">
                                                <div class="progress-bar" role="progressbar" style="width: {{ (price/s)*10 }}%" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100"></div>
                                            </div>
                                        </div>
                                        <div class="col-xl-2 col-lg-3 col-3"><strong>{{ (price/s)|number_format('1') }}</strong></div>
                                    </div>
                                    <!-- /row -->
                                </div>
                            </div>
                            <!-- /row -->
                        </div>
                    </div>
                    <!-- /row -->
                    {% for r in reclamation %}
                    <div id="reviews">
                        <div class="review_card">
                            <div class="row">
                                <div class="col-md-2 user_info">
                                    <figure><img src="/img/avatar4.jpg" alt=""></figure>
                                    <h5>Lukas</h5>
                                </div>
                                <div class="col-md-10 review_content">
                                    <div class="clearfix add_bottom_15">
                                        <span class="rating">{{ ((r.foodqulaite+r.service+r.price)/3)|number_format('1')}}<small>/10</small> <strong>Rating average</strong></span>

                                        <em>le :{{r.daterec|date('d/m/Y h:m:s')}}</em>
                                    </div>
                                    <h4>{{ r.titre }}</h4>
                                    <p>{{ r.description }}</p>
                                    {% if r.nomimage is not null %}
                                    <div class="col-md-6">
                                        <figure><img src="{{ asset('/uploads/' ~ r.nomimage )  }}"  height="200px" width="200px" class="lazy"/></figure>
                                    </div>
                                    {% endif %}
                                    <ul>
                                        <li><a href="{{ path ('addReply',{'id':r.id}) }}"><i class=""></i><span>répondre</span></a></li>
                                        <li><a href="{{ path ('update',{'id':r.id}) }}"><i class=""></i><span>Modifier</span></a></li>
                                        <li><a href="{{ path ('deleteR',{'id':r.id}) }}"><i class=""></i><span>Supprimer</span></a></li>
                                        <li></li>
                                        {% if r.getReplies is not empty %}
                                        <li><a href="{{ path ('afficheReply',{'id':r.id}) }}"><i class="arrow_back"></i> <span>afficher les réponses</span></a></li>
                                    {% endif %}
                                        {% if r.getReplies is  empty %}
                                           <li><span>Reclamation Non traitée</span></li>
                                        {% endif %}
                                    </ul>
                                </div>

                                <!-- /row -->
                            </div>
                            <!-- /review_card -->



                            <!-- /reply -->
                        </div>
                        <!-- /review_card -->
                    </div>
                    <!-- /reviews -->
                    {% endfor %}

                    <div class="text-right"><a href="{{ path ('addReclamation') }}" class="btn_1 gradient">Ajouter une reclamation</a></div>
                </section>
                <!-- /section -->
            </div>
